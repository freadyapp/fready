        "babel-jest": "^26.6.3",
        "babelify": "^10.0.0",
        "browserify-css": "^0.15.0",
        "css-loader": "^5.0.1",
        "eslint": "^7.19.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
